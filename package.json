{
  "name": "just-notes",
  "packageManager": "yarn@3.3.0",
  "version": "0.1.0",
  "description": "Sample notes aplication",
  "private": true,
  "workspaces": [
    "api",
    "web"
  ],
  "scripts": {
    "start": "npm-run-all --parallel api web",
    "api": "bash -c 'cd api && yarn start'",
    "web": "bash -c 'cd web && yarn web'",
    "test": "yarn node ./tests/run.js"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "^18.0.0",
    "chokidar": "^3.5.3",
    "eslint": "5.12.0",
    "jest": "29.4.3",
    "jest-pnp-resolver": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "swagger-ui-dist": "git+https://github.com/TexKiller/swagger-ui#in-use"
  },
  "dependenciesMeta": {
    "jest@29.4.3": {
      "unplugged": true
    },
    "swagger-ui-dist": {
      "unplugged": true
    }
  },
  "dependencies": {
    "react-router-dom": "^6.11.1"
  }
}
